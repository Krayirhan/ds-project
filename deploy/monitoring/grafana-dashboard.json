{
  "title": "DS Project API Overview",
  "timezone": "browser",
  "panels": [
    {
      "type": "timeseries",
      "title": "Request Rate",
      "targets": [
        {
          "expr": "sum(rate(ds_api_requests_total[1m]))",
          "legendFormat": "req/s"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "p95 Latency (seconds)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(ds_api_request_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "5xx Rate",
      "targets": [
        {
          "expr": "sum(rate(ds_api_requests_total{status=~\"5..\"}[5m]))",
          "legendFormat": "5xx/s"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Inference Errors",
      "targets": [
        {
          "expr": "sum(rate(ds_api_inference_errors_total[5m]))",
          "legendFormat": "errors/s"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s"
}
